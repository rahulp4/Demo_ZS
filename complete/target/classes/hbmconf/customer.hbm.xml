<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.demo.fileproc.service.dto.customer.CustomerEntityDTO" table="DEMO_CUSTOMER">
   		<id name="id" column="id">
            
        </id>

        <property name="fileRefId" type="string">
            <column name="fileRefId" />
        </property>

        <property name="name" type="string">
            <column name="name" length="50" />
        </property>
        <property name="address" type="string">
            <column name="address" length="30" />
        </property>
        <property name="attribute1" type="string">
            <column name="attribute1" length="30" />
        </property>
        <property name="attribute2" type="string">
            <column name="attribute2" length="30" />
        </property>
	        
    </class>
</hibernate-mapping>
