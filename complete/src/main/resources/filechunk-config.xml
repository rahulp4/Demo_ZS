<?xml version="1.0" encoding="UTF-8"?>
<cache-config>
   <caching-scheme-mapping>
      <cache-mapping>
         <cache-name>filechunkcoherenace</cache-name>
         <scheme-name>filechunkcoherenacedistributed</scheme-name>
      </cache-mapping>
      <cache-mapping>
      		<cache-name>entitycoherenace</cache-name>
         	<scheme-name>entitycoherenacedistributed</scheme-name>
      </cache-mapping>
   </caching-scheme-mapping>
   
   <caching-schemes>
   
      <distributed-scheme>
         <scheme-name>filechunkcoherenacedistributed</scheme-name>
         <service-name>DistributedCache</service-name>
         <backing-map-scheme>
         	<local-scheme/>
         </backing-map-scheme>
         <autostart>true</autostart>
      </distributed-scheme>
      
      <distributed-scheme>
         <scheme-name>entitycoherenacedistributed</scheme-name>
         <service-name>DistributedCache</service-name>
         <backing-map-scheme>
         	<local-scheme/>
         </backing-map-scheme>
         <autostart>true</autostart>
      </distributed-scheme>      
   </caching-schemes>
</cache-config>